<UserControl x:Class="FastHelpClientView.MainPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:myviews="clr-namespace:FastHelpClientView.Views"
             xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
             mc:Ignorable="d">

    <Grid x:Name="LayoutRoot"  VerticalAlignment="Top" 
          Style="{StaticResource LayoutRootGridStyle}"
          Background="White">
        
        <Border x:Name="ContentBorder"
                Style="{StaticResource ContentBorderStyle}">
            
            <navigation:Frame x:Name="ContentFrame"
                              Source="/Views/MainMenu.xaml"
                              Width="802"
                              Height="600"
                              Template="{StaticResource TransitioningFrame}">

            </navigation:Frame>
        </Border>
       
        
        
    </Grid>


</UserControl>
